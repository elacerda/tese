	Date-Added = {2014-07-10 14:38:13 +0000},
	Date-Modified = {2014-07-10 14:38:13 +0000},
	Doi = {10.1111/j.1365-2966.2005.09935.x},
	Eprint = {astro-ph/0512042},
	Journal = {\mnras},
	Keywords = {dust, extinction-galaxies: abundances-galaxies: ISM-quasars: absorption lines},
