@article{Guiderdoni.Rocca.1987,
	Abstract = {The authors present a model of spectrophotometric evolution which predicts apparent magnitudes and colors for galaxies of the Hubble sequence at all redshifts. This model accounts for the effects of cosmology and intrinsic evolution which can thereby be separated. It includes the four main phases of stellar evolution (main sequence, giant branch, horizontal branch, and asymptotic giant branch). The library of 30 stellar spectra (from 220 {\AA} to 10,680 {\AA}) which is used for the evolutionary synthesis is basically observational, with a resolution of 10 {\AA}. The contribution of nebular emission and the effects of internal extinction are taken into account. The model gives good fits of far-UV and visible spectrophotometric properties of nearby galaxies along the Hubble sequence, by varying the timescale for the conversion of gas into stars, with uniform IMF.},
