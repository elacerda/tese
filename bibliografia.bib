	Journal = {\apj},
	Keywords = {Compact Galaxies, Star Distribution, Star Formation, Stellar Spectra, Ultraviolet Spectra, Absorption Spectra, Dwarf Galaxies, Main Sequence Stars, Stellar Luminosity, Supergiant Stars},
	Mnrascitekey = Fanelli.OConnell.Thuan.1988,
