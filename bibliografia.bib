	Eprint = {astro-ph/0404103},
	Journal = {\apjs},
	Keywords = {Galaxies: Fundamental Parameters, Galaxies: Photometry, Galaxies: Statistics, Galaxies: Structure, Techniques: Image Processing, Techniques: Photometric},
	Mnrascitekey = deSouza.Gadotti.dosAnjos.2004,
