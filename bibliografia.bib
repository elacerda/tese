	Date-Added = {2015-06-02 10:49:46 +0000},
	Date-Modified = {2015-06-02 10:50:25 +0000},
	Doi = {10.1093/mnras/stu1288},
	Eprint = {1402.5964},
	Journal = {\mnras},
	Keywords = {galaxies: evolution, galaxies: formation, galaxies: fundamental parameters, galaxies: high-redshift},
	Mnrascitekey = Peng.Maiolino.2014,
	Month = oct,
	Pages = {3643-3664},
	Title = {{From haloes to Galaxies - I. The dynamics of the gas regulator model and the implied cosmic sSFR history}},
