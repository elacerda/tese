	Doi = {10.1086/344301},
	Eprint = {astro-ph/0209012},
	Journal = {\apj},
	Keywords = {Galaxies: Abundances, Galaxies: Evolution, Galaxies: Irregular, Galaxies: Spiral},
	Mnrascitekey = Garnett.2002,
	Month = dec,
	Pages = {1019-1031},
	Title = {{The Luminosity-Metallicity Relation, Effective Yields, and Metal Loss in Spiral and Irregular Galaxies}},
